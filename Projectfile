# bonobo_quirksmode (see github.com/python-medikit)

from medikit import require

pytest = require('pytest')
python = require('python')
yapf = require('yapf')

python.setup(
    name='bonobo_quirksmode',
    description='Some quirks workarounds I do not want to push in the main repo (yet, or maybe never).',
    license='Apache License, Version 2.0',
    url='',
    download_url='',
    author='Romain Dorgueil',
    author_email='romain@dorgueil.net',
)

bonobo_version = '~= 0.6.0'
python.add_requirements(
    'bonobo ' + bonobo_version,
)

